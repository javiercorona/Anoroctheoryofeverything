\section{Core Framework}

\subsection{Gravitational Sector}
\begin{equation}
G_{\mu\nu} + f(K)\mathcal{H}_{\mu\nu} + \mathcal{V}_{\mu\nu} = \kappa f(K)T_{\mu\nu}^{(\text{string})}
\end{equation}

\begin{itemize}
    \item \textbf{Curvature Regulation}:
    $f(K) = 1 - e^{-K/K_{\text{max}}}$ (exponential cutoff)
    
    \item \textbf{String Coupling}:
    $T_{\mu\nu}^{(\text{string})} = -\frac{2}{\sqrt{-g}}\frac{\delta S_{\text{String}}}{\delta g^{\mu\nu}}$
    
    \item \textbf{Quantum Backreaction}:
    $\mathcal{V}_{\mu\nu} = \mel{0}{\hat{T}_{\mu\nu}^{(\text{string})}}{0}_{\text{1-loop}}$
\end{itemize}

\subsection{Parameter Space}
\begin{table}[h]
