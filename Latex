\section{Key Features}

\subsection{Unification Mechanism}
\begin{itemize}
    \item \textbf{4D Covariance}: No compactified dimensions
    \item \textbf{UV/IR Mixing}: $\ell_s$ sets both string scale and curvature cutoff
    \item \textbf{Dual Protection}:
    \begin{itemize}
        \item ANOROC: $f(K)$ regulates curvature
        \item Strings: T-duality avoids $\ell_s$ singularities
    \end{itemize}
\end{itemize}

\subsection{Experimental Tests}
\begin{equation*}
\begin{aligned}
&\text{GW Ringdown}: \frac{\Delta\omega}{\omega_{\text{GR}}} \sim 1 - e^{-K/K_{\text{max}}} \\
&\text{Colliders}: \sigma \sim \exp\left[-\left(\frac{E}{E_s}\right)^2\right],\ E_s = \hbar c/\ell_s
\end{aligned}
\end{equation*}

